            "identifier": "lookup_domain",
            "read_only": true,
            "parameters": {
                "domain": {
                    "description": "Domain to query",
                    "data_type": "string",
                    "order": 0,
                    "primary": true,
                    "required": true,
                    "contains": [
                        "domain"
                    ]
                },
                "type": {
                    "description": "Type of domain lookup",
                    "data_type": "string",
                    "order": 1,
                    "primary": true,
                    "value_list": [
                        "mx",
                        "a",
                        "dns",
                        "spf",
                        "txt",
                        "soa"
                    ],
                    "required": true
                }
            },
            "render": {
                "width": 12,
                "title": "Lookup Domain",
                "type": "json",
                "height": 5
            },
